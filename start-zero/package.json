{
  "name": "start-zero",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vinxi build",
    "check": "biome check",
    "clean": "rm -rf node_modules bun.lock",
    "db:clean": "source .env && docker volume rm -f start_zero_pgdata && rm -rf \"${ZERO_REPLICA_FILE:-/dev/null}\"*",
    "db:down": "docker compose --env-file .env -p start_zero -f docker/docker-compose.yml down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts",
    "auth:studio": "drizzle-kit studio --config auth-drizzle.config.ts",
    "db:up": "docker compose -p start_zero -f docker/docker-compose.yml up --remove-orphans",
    "db:zero:generate": "drizzle-zero generate --format --output src/db/zero-schema.gen.ts",
    "dev": "vinxi dev",
    "format": "biome format",
    "lint": "biome lint",
    "serve": "vite preview",
    "shadcn": "bunx shadcn@canary add",
    "start": "vinxi start",
    "test": "vitest run",
    "update-packages": "bunx taze -I --recursive",
    "zero-cache": "source .env && zero-cache-dev -p src/db/zero-schema.gen.ts --log-level debug",
    "db:auth:generate": "drizzle-kit generate --config drizzle.config.auth.ts",
    "db:auth:push": "drizzle-kit push --config drizzle.config.auth.ts",
    "db:auth:migrate": "drizzle-kit migrate --config drizzle.config.auth.ts",
    "db:auth:studio": "drizzle-kit studio --config drizzle.config.auth.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@rocicorp/zero": "~0.19.2025050203",
    "@tabler/icons-react": "^3.31.0",
    "@tailwindcss/postcss": "^4.1.5",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/react-form": "^1.9.0",
    "@tanstack/react-query": "^5.75.2",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "^1.119.1",
    "@tanstack/react-router-with-query": "^1.119.0",
    "@tanstack/react-start": "^1.119.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.119.0",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "better-auth": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.40.1",
    "drizzle-zero": "^0.9.2",
    "lucide-react": "^0.503.0",
    "next-themes": "^0.4.6",
    "pg": "^8.15.6",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.2",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "vinxi": "^0.5.6",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.4"
  },
  "devDependencies": {
