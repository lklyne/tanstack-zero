{
  "name": "start-zero",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vinxi build",
    "check": "biome check",
    "clean": "rm -rf node_modules bun.lock",
    "db:clean": "source .env && docker volume rm -f start_zero_pgdata && rm -rf \"${ZERO_REPLICA_FILE:-/dev/null}\"*",
    "db:down": "docker compose --env-file .env -p start_zero -f docker/docker-compose.yml down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "auth:studio": "drizzle-kit studio --config auth-drizzle.config.ts",
    "db:up": "docker compose -p start_zero -f docker/docker-compose.yml up --remove-orphans",
    "dev": "vinxi dev",
    "format": "biome format",
    "lint": "biome lint",
    "serve": "vite preview",
    "shadcn": "bunx shadcn@canary add",
    "start": "vinxi start",
    "test": "vitest run",
    "update-packages": "bunx taze -I --recursive",
    "zero-cache": "source .env && zero-cache-dev -p src/db/schema.zero.ts --log-level debug",
    "db:auth:generate": "drizzle-kit generate --config drizzle.config.auth.ts",
    "db:auth:push": "drizzle-kit push --config drizzle.config.auth.ts",
    "db:auth:studio": "drizzle-kit studio --config drizzle.config.auth.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@rocicorp/zero": "^0.16.2025022800",
    "@tailwindcss/postcss": "^4.0.12",
    "@tailwindcss/vite": "^4.0.12",
    "@tanstack/react-form": "^1.0.5",
    "@tanstack/react-query": "^5.71.0",
    "@tanstack/react-router": "^1.114.4",
    "@tanstack/react-router-devtools": "^1.114.4",
    "@tanstack/react-router-with-query": "^1.114.29",
    "@tanstack/react-start": "^1.114.4",
    "@tanstack/router-plugin": "^1.114.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.40.0",
    "drizzle-zero": "^0.5.2",
    "lucide-react": "^0.479.0",
    "next-themes": "^0.4.6",
    "pg": "^8.13.3",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "sonner": "^2.0.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.12",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "^0.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^9.6.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "^0.30.5",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vitest": "^3.0.8",
    "web-vitals": "^4.2.4"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@rocicorp/zero-sqlite3",
    "esbuild"
  ]
}
